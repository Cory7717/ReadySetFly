{
  "expo": {
    "name": "rsf_app",
    "slug": "rsf_app",
    "version": "1.0.0",
    "scheme": "scheme",
    "orientation": "portrait",
    "icon": "./Assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./Assets/images/fulllogo_transparent.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["assets/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.readysetfly.rsf",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "This app needs access to your photo library to allow you to share photos with your friends."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./Assets/icon.png",
        "backgroundImage": "./Assets/images/fulllogo_transparent.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.readysetfly.rsf",
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "CAMERA"
      ]
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "@react-native-google-signin/google-signin",
      "expo-document-picker",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "jsEngine": "hermes"
  }
}
