{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src/**/*.ts", "src/**/*.tsx"]
,
  "exclude": [
    "node_modules",
    "dist",
    "**/*.spec.ts"
  ],
  "extends": "expo/tsconfig.base"
}
